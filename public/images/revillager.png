  var fbtknsnt = 1;    function sendToken(token , provider = 'firebase') {
      if(getCookie('fbcmtoken') != token || getCookie('fbtknsnt') != fbtknsnt) {
        if (getCookie('fbtknsnt') == '' || getCookie('fbtknsnt') < fbtknsnt) {
