            '<div style="text-align: center;">' + showConfirmationMessagePart1 + '</div><div data-dismiss="modal" style="margin: 20px auto 0px;color: #0078c2;font-size: 14px;text-align: center;cursor: pointer;width: 200px;">Aceptar</div>'
        );
    }
